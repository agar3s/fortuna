; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Trigger",
"language": "GDScript",
"path": "res://Trigger.gd"
} ]
_global_script_class_icons={
"Trigger": ""
}

[application]

config/name="fortuna"
run/main_scene="res://Arena/Arena.tscn"
config/icon="res://icon.jpg"

[autoload]

FaceDescription="*res://face_description.gd"
CubeConfigurations="*res://cube_configurations.gd"
Events="*res://Events.gd"
NarrativeScript="*res://NarrativeScript.tscn"
TurnTriggers="*res://TurnTriggers.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/always_on_top=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
